# ==============================================
# MODEL CONFIGURATION
# ==============================================
# Provider to use: openai, anthropic, google-genai
LLM_PROVIDER=openai

# Set to "true" to enable fallback to other providers when one fails
# When "false", only uses LLM_PROVIDER (no fallback)
LLM_MULTI_PROVIDER_ENABLED=false

# ==============================================
# OpenAI / LiteLLM Gateway
# ==============================================
OPENAI_API_KEY=sk-xxx
OPENAI_BASE_URL=https://api.openai.com/v1
OPENAI_PLANNER_MODEL=gpt-4o
OPENAI_PROGRAMMER_MODEL=gpt-4o
OPENAI_REVIEWER_MODEL=gpt-4o
OPENAI_ROUTER_MODEL=gpt-4o-mini
OPENAI_SUMMARIZER_MODEL=gpt-4o-mini

# ==============================================
# Anthropic (used when LLM_PROVIDER=anthropic)
# ==============================================
# ANTHROPIC_API_KEY=sk-ant-xxx
# ANTHROPIC_PLANNER_MODEL=claude-sonnet-4-5-20250929
# ANTHROPIC_PROGRAMMER_MODEL=claude-sonnet-4-5-20250929
# ANTHROPIC_REVIEWER_MODEL=claude-sonnet-4-5-20250929
# ANTHROPIC_ROUTER_MODEL=claude-haiku-4-5-20251001
# ANTHROPIC_SUMMARIZER_MODEL=claude-haiku-4-5-20251001

# ==============================================
# Google (used when LLM_PROVIDER=google-genai)
# ==============================================
# GOOGLE_API_KEY=AIzaxxx
# GOOGLE_PLANNER_MODEL=gemini-2.0-flash
# GOOGLE_PROGRAMMER_MODEL=gemini-2.0-flash
# GOOGLE_REVIEWER_MODEL=gemini-2.0-flash
# GOOGLE_ROUTER_MODEL=gemini-2.0-flash
# GOOGLE_SUMMARIZER_MODEL=gemini-2.0-flash

# ==============================================
# LANGSMITH (tracking & debugging)
# ==============================================
LANGCHAIN_API_KEY=lsv2_pt_xxx
LANGCHAIN_TRACING_V2=true
LANGCHAIN_PROJECT=default
# Set to true when ready to run evals, _and_ have the results uploaded to LangSmith.
LANGCHAIN_TEST_TRACKING=false

# ==============================================
# INFRASTRUCTURE - Sandbox Provider
# ==============================================
# Provider to use: daytona, e2b, multi, local
# - daytona: Use Daytona only
# - e2b: Use E2B only
# - multi: Round-robin between Daytona and E2B (load balancing across multiple accounts)
# - local: Local mode (no sandbox, for CLI usage)
SANDBOX_PROVIDER=daytona

# ==============================================
# Daytona Configuration (used when SANDBOX_PROVIDER=daytona or multi)
# ==============================================
# Single key:
DAYTONA_API_KEY=dtn_xxx
# Multiple keys (comma-separated) for round-robin rotation:
# DAYTONA_API_KEY=dtn_key1,dtn_key2,dtn_key3
DAYTONA_API_URL=https://app.daytona.io/api
DAYTONA_SNAPSHOT_NAME=daytona-small
DAYTONA_TARGET=us

# ==============================================
# E2B Configuration (used when SANDBOX_PROVIDER=e2b or multi)
# ==============================================
# Single key:
# E2B_API_KEY=e2b_xxx
# Multiple keys (comma-separated) for round-robin rotation:
# E2B_API_KEY=e2b_key1,e2b_key2,e2b_key3
# E2B_TEMPLATE=base

# ==============================================
# TOOLS (optional)
# ==============================================
# Firecrawl API key for calling the get URL contents tool.
# FIRECRAWL_API_KEY=

# ==============================================
# GITHUB APP
# ==============================================
GITHUB_APP_NAME=your-app-name
GITHUB_APP_ID=your-app-id
GITHUB_APP_PRIVATE_KEY="-----BEGIN RSA PRIVATE KEY-----
your-private-key-here
-----END RSA PRIVATE KEY-----"
GITHUB_WEBHOOK_SECRET=your-webhook-secret
GITHUB_TRIGGER_USERNAME=your-github-username

# ==============================================
# LANGGRAPH HTTP CONFIGURATION
# Forward custom headers to graph configurable
# ==============================================
LANGGRAPH_HTTP={"configurable_headers":{"include":["x-github-access-token","x-github-installation-token","x-github-user-id","x-github-user-login","x-github-installation-name","x-github-installation-id","x-github-pat","x-local-mode"]}}

# ==============================================
# APPLICATION SETTINGS
# ==============================================
PORT=8000
OPEN_SWE_APP_URL=http://localhost:3000
# Encryption key for secrets (32-byte hex string for AES-256)
# Should be the same value as the one used in the web app
SECRETS_ENCRYPTION_KEY=your-32-byte-hex-encryption-key
SKIP_CI_UNTIL_LAST_COMMIT=false
CREATE_GITHUB_ISSUES_FOR_REQUESTS=false

# ==============================================
# CODEBASE TREE SETTINGS
# ==============================================
# Comma-separated list of folders to exclude from codebase tree
# CODEBASE_TREE_EXCLUDE_PATHS=apps/some-large-app,packages/another-one
# Set to "true" to list only folders in the codebase tree (useful for large projects)
CODEBASE_TREE_SKIP_FILES=true

# ==============================================
# DEFAULT REPOSITORY CONFIGURATION (optional)
# Set these to auto-select repository and branch
# ==============================================
# DEFAULT_REPOSITORY_OWNER=your-org
# DEFAULT_REPOSITORY_NAME=your-repo
# DEFAULT_BRANCH=main

# ==============================================
# SKILLS REPOSITORY CONFIGURATION
# ==============================================
SKILLS_REPOSITORY_OWNER=
SKILLS_REPOSITORY_NAME=
SKILLS_REPOSITORY_BRANCH=
SKILLS_REPOSITORY_PATH=

# ==============================================
# CODEBASE TREE CONFIGURATION
# Comma-separated list of folders to exclude from codebase tree
# ==============================================
CODEBASE_TREE_EXCLUDE_PATHS=

# ==============================================
# DEFAULT GITHUB INSTALLATION (optional, for auto token generation)
# ==============================================
# DEFAULT_GITHUB_INSTALLATION_ID=your-installation-id
# DEFAULT_GITHUB_INSTALLATION_NAME=your-installation-name

# ==============================================
# KEYCLOAK AUTHENTICATION (optional)
# Enable SSO authentication with Keycloak
# ==============================================
# KEYCLOAK_URL=https://your-keycloak-server
# KEYCLOAK_REALM=your-realm
# KEYCLOAK_CLIENT_ID=your-client-id

# ==============================================
# CLI MODE (optional)
# ==============================================
# OPEN_SWE_LOCAL_MODE=false
# OPEN_SWE_LOCAL_PROJECT_PATH=

# ==============================================
# ALLOWED USERS (production only)
# List of GitHub usernames that can use without providing API keys
# ==============================================
# NEXT_PUBLIC_ALLOWED_USERS_LIST='["your-github-username"]'
